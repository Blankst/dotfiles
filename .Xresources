URxvt*scrollBar:                      false
URxvt*mouseWheelScrollPage:           true
URxvt*font:                           xft:terminus:size=10
URxvt*boldFont:                       xft:terminus:bold:size=10
URxvt*saveLines:                      9999
URxvt*depth:                          32
URxvt*borderLess:                     false
URxvt*letterSpace:                    -0.5
URxvt*perl-ext-common:                vtwheel,default,matcher
URxvt*matcher.button:                 1
URxvt*url-launcher:                   /usr/bin/firefox -new-tab
URxvt*internalborder:    	      8
URxvt*cursorBlink:			true

*cursorColor:   #666666

*background:    [99]#101010
*foreground:    #c5c5c5

! black
*color0:        #1C1C1C
*color8:        #666666

! red
*color1:        #ff005b
*color9:        #ff00a0

! green
*color2:        #cee318
*color10:       #ccff00

! yellow
*color3:        #ffe755
*color11:       #ff9f00

! blue
*color4:        #048ac7
*color12:       #48c6ff

! magenta
*color5:        #833c9f
*color13:       #be67e1

! cyan 
*color6:        #0ac1cd
*color14:       #63e7f0

! white
*color7:        #e5e5e5
*color15:       #f3f3f3
